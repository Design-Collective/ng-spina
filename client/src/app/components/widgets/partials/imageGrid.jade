.image-box(ng-repeat='person in iGrid.gridData', ng-style='iGrid.getBackground(person)')
    .box-content
        .description
            p.socials
                a(ng-repeat='social in person.socials', ng-href='social.link')
                    i(ng-class='social.iconClass')
            p.position {{ person.position }}
            h3.title {{ person.title }}